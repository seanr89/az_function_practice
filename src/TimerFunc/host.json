{
    "version": "2.0",
    "logging": {
        "logLevel": {
            "default": "Warning",
            "Function": "Error",
            "Host.Aggregator": "Error",
            "Host.Results": "Information",
            "Microsoft.EntityFrameworkCore.Database": "Warning"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    }
}